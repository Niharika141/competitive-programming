class Solution:
    def findLongestChain(self, pairs: List[List[int]]) -> int:
        pairs.sort(key=lambda x:x[1])
        c=1
        prev=pairs[0][1]
        i=1
       
        while i<len(pairs):
            if pairs[i][0]>prev:
                c+=1
                prev=pairs[i][1]
            i+=1
        return c
